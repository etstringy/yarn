{
  "name": "yarn",
  "version": "3.0.0",
  "description": "A discord bot.",
  "main": "index.js",
  "author": "etstringy",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "yarn && yarn dbgen && tsnd --exit-child --respawn --poll --interval 1000 src/index.ts",
    "start": "yarn && yarn dbgen && yarn prisma migrate deploy && yarn build && node ./dist/src/index",
    "dbgen": "yarn prisma generate",
    "build": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^3.4.0",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "discord.js": "^13.3.1",
    "dotenv": "^8.2.0",
    "figlet": "^1.5.2",
    "gradient-string": "^1.2.0",
    "lodash": "^4.17.21",
    "ora": "^5.4.0",
    "prisma": "3.4.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.4",
    "@types/gradient-string": "^1.1.2",
    "@types/lodash": "^4.14.171",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.4.3",
    "@types/node-fetch": "^2.5.7",
    "@types/ws": "^7.4.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.1"
  },
  "prisma": {
    "schema": "src/db/schema.prisma"
  }
}
