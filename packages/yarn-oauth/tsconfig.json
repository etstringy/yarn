{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "target": "ES2020",
      "noImplicitAny": true,
      "strict": true,
      "moduleResolution": "node",
      "sourceMap": false,
      "outDir": "dist",
      "resolveJsonModule": true,
      "strictNullChecks": false
  }
}